// what we need back (only the bits we actually read)
@Data
public class AccountHolderListResponse {
  private List<ProductAccount> data;
  private Status status;
  private TraceInfo traceInfo;

  @Data public static class ProductAccount {
    private String productTypeCode;
    private String productReferenceNumber;
    private List<AccountHolder> accountHolders;
  }

  @Data public static class AccountHolder {
    private String cmcpId;
    private String cin;
    private String cinSuffix;
    private Integer customerType;
    private Integer jointAccountOrder;
    private String registeredName;
    private String nameLine1;
  }

  @Data public static class Status { private int statusCode; }
  @Data public static class TraceInfo { private String traceId; private String spanId; private String timestamp; }
}
