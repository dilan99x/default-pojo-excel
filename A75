@PostMapping(value="/vX/â€¦/rss", produces=MediaType.APPLICATION_JSON_VALUE)
public ResponseEntity<StreamingResponseBody> getRss(
    @RequestBody CasaAccountSummaryRequest req,
    @RequestHeader HttpHeaders headers) {

  StreamingResponseBody srb = os -> crrsService.writeRssResponse(os, req, headers);
  return ResponseEntity.ok()
      .contentType(MediaType.APPLICATION_JSON)
      .body(srb);
}
